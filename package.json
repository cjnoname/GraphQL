    "history": "4.7.2",
    "immutable": "4.0.0-rc.9",
    "node-fetch": "2.2.0",
    "node-noop": "1.0.0",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-hot-loader": "4.3.4",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
